{
    "name": "kpeu3i/jwt-bundle",
    "type": "symfony-bundle",
    "description": "Integrates JWT authentication mechanism into Symfony applications",
    "keywords": ["symfony", "jwt", "jws"],
    "homepage": "https://github.com/kpeu3i/jwt-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Bukatov",
            "email": "andrey.bukatov@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.8",
        "symfony/framework-bundle": "~2.3|~3.0",
        "symfony/security-bundle": "~2.3|~3.0",
        "symfony/console": "~2.3|~3.0",
        "lcobucci/jwt": "3.*"
    },
    "autoload": {
        "psr-0": { "Kpeu3i\\JwtBundle\\": "" }
    },
    "target-dir": "Kpeu3i/JwtBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.1|^5.0",
        "symfony/phpunit-bridge": "~2.7|~3.0"
    }
}
